{
  "ConnectionStrings": {
    "TodoConnection": "Host=localhost;Port=5432;Database=todo_db;Username=admin;Password=Zaqwsx1@"
  },
  "Keycloak": {
    "realm": "SimpleTodo",
    "auth-server-url": "http://localhost:8085",
    "ssl-required": "none",
    "resource": "simple-todo-ui",
    "verify-token-audience": false,
    "credentials": {
      "secret": ""
    },
    "confidential-port": 0
  },
  "Serilog": {
    "Properties": {
      "ApplicationName": "SimpleTodo"
    },
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console",
      "Serilog.Expressions"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj} <{ThreadId}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log_.log",
          "rollingInterval": "Day",
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj} <{ThreadId}>{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "WithThreadId",
      "FromConfiguration",
      "FromLogContext"
    ]
  }
}
